{
	"id": "autocomplete",
	"version": "0.1",
	"elementPrefix": "aseeco",
	"propertyPrefix": "",
	"supports": {
		"javascript": "https://npmjs.com/package/adaptivecards-autocomplete"
	},
	"contributes": {
		"definitions": {
			"FilterType": {
				"classType": "Enum",
				"type": "FilterType",
				"values": [
					"none",
					"startsWith",
					"includes",
					"endsWith"
				]
			},
			"ChoiceInputStyle": {
				"classType": "Enum",
				"type": "ChoiceInputStyle",
				"values": [
					"compact",
					"expanded",
					"autocomplete"
				]
			},
			"Input.ChoiceSet": {
				"properties": {
					"filterType": {
						"type": "FilterType",
						"description": "Autocomplete filter type",
						"version": "0.1",
						"default": "startsWith"
					},
					"showClear": {
						"type": "boolean",
						"description": "Show clear button when style is autocomplete",
						"version": "0.1",
						"default": true
					},
					"style": {
						"type": "ChoiceInputStyle",
						"description": "Style hint for `Input.ChoiceSet`.",
						"version": "0.1",
						"default": "compact"
					}
				}
			}
		}
	}
}