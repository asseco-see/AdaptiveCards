{
	"id": "classification-select",
	"version": "0.1",
	"elementPrefix": "",
	"propertyPrefix": "asseco",
	"contributes": {
		"definitions": {
		
			"Input.Classification": {
				"extends": "Input",
				"description": "Allows user to select a classification from a service",
				"properties": {
					"dataSource": {
						"type": "DataSource",
						"description": "Classification data source"
					},
					"multiple": {
						"type": "boolean",
						"description": "Multiple selections allowed when allowAutocomplete is false",
						"version": "0.1",
						"default": false
					},
					"value": {
						"type": "string",
						"description": "The initial value for this field.",
						"version": "0.1"
					},
					"allowAutocomplete": {
						"type": "boolean",
						"description": "Allow autocomplete control",
						"version": "0.1",
						"default": false
					},
					"filterType": {
						"type": "FilterType",
						"description": "Filter type when allowAutocomplete is true",
						"version": "0.1",
						"default": "startsWith"
					}
				}
			}
		}
	}
}