{
	"id": "material-dialog",
	"version": "0.1",
	"elementPrefix": "",
	"propertyPrefix": "asseco",
	"contributes": {
		"definitions": {
			"MaterialDialogConfig": {
				"type": "object",
				"description": "Controls dialogs styling.",
				"version": "0.1",
				"properties": {
					"actionsOrientation": {
						"type": "string",
						"description": "Controls how buttons are laid out in dialog.",
						"default": "horizontal",
						"enum": [
							"horizontal",
							"vertical"
						]
					},
					"actionsAlignment": {
						"type": "string",
						"description": "Control layout of buttons in dialog.",
						"default": "stretch",
						"enum": [
							"left",
							"center",
							"right",
							"stretch"
						]
					}
				}
			}
		}
	},
	"hostConfig": {
