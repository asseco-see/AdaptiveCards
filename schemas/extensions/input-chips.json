{
    "id": "textinput-checkdigit-extension",
    "version": "0.1",
    "elementPrefix": "",
    "propertyPrefix": "asseco",
    "supports": {
        "javascript": "https://npmjs.com/package/adaptivecards-autocomplete"
    },
    "contributes": {
	    "definitions": {
			"Input.Chip": {
				  "description": "Describes a chip for use in a ChipSet.",
				  "properties": {
					"type": {
					  "enum": [
						"Input.Chip"
					  ],
					  "description": "Must be `Input.Chip`"
					},
					"id": {
					  "type": "string",
					  "description": "Unique identifier for chip.",
					  "version": "1.4"
					},
					"name": {
					  "type": "string",
					  "description": "Chip name.",
					  "version": "1.4"
					},
					"selected": {
					  "type": "boolean",
					  "description": "Whether or not the chip is selected. Default value is false",
					  "version": "1.4"
					}
				  },
				  "type": "object",
				  "additionalProperties": false,
				  "required": [
					"id",
					"name",
					"selected"
				  ]
			 },
			 "Input.Chips": {
				  "description": "Allows a user to input a Chip.",
				  "properties": {
					"type": {
					  "enum": [
						"Input.Chips"
					  ],
					  "description": "Must be `Input.Chips`"
					},
					"chips": {
					  "type": "array",
					  "items": {
						"$ref": "#/definitions/Input.Chip"
					  },
					  "description": "`Chip` options.",
					  "version": "1.4"
					},
					"value": {
					  "type": "string",
					  "description": "The initial choice (or set of chips) that should be selected. For multi-select, specify a comma-separated string of values.",
					  "version": "1.4"
					},
					"placeholder": {
					  "type": "string",
					  "description": "Description of the input desired. Only visible when no selection has been made, the `style` is `compact` and `isMultiSelect` is `false`",
					  "version": "1.4"
					},
					"color": {
					  "type": "string",
					  "description": "Material color palette of chip. Initial is 'primary'.",
					  "version": "1.4"
					},
					"selectable": {
					  "type": "boolean",
					  "description": "Whether or not chips are selectable. Default is true.",
					  "version": "1.4"
					},
					"removable": {
					  "type": "boolean",
					  "description": "Whether or not chips are removable. Default is true.",
					  "version": "1.4"
					},
					"addOnBlur": {
					  "type": "boolean",
					  "description": "Whether or not the chipEnd event will be emitted when the input is blurred. Default is true.",
					  "version": "1.4"
					},
					"id": {},
					"errorMessage": {},
					"isRequired": {},
					"label": {},
					"fallback": {},
					"height": {},
					"separator": {},
					"spacing": {},
					"isVisible": {},
					"requires": {}
				  },
				  "type": "object",
				  "additionalProperties": false,
				  "required": [
					"chips"
				  ],
				  "allOf": [
					{
					  "$ref": "#/definitions/Extendable.Input"
					}
				  ]
			}
        }
    }
}