{
    "id": "textinput-checkdigit-extension",
    "version": "0.1",
    "elementPrefix": "",
    "propertyPrefix": "asseco",
    "supports": {
        "javascript": "https://npmjs.com/package/adaptivecards-autocomplete"
    },
    "contributes": {
	    "definitions": {
			"InputTextCheckDigitAlgorithm": {
			  "description": "Check digit algorithm",
			  "anyOf": [
				{
				  "enum": [
					"mod11",
					"mod97"
				  ]
				},
				{
				  "pattern": "^([m|M][o|O][d|D][1|1][1|1])|([m|M][o|O][d|D][9|9][7|7])$"
				}
			  ]
		    },
            "Input.Text": {
                "properties": {
                      "checkDigit": {
						  "$ref": "#/definitions/InputTextCheckDigitAlgorithm",
						  "description": "The initial value for this field."
					  }
                }
            }
        }
    },
    "hostConfig": {
        "properties": {
            "registries": {
                "type": "array",
                "description": "The list of registries to lookup templates",
                "items": {
                    "type": "uri"
                }
            }
        }
    }
}