{
  "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/6f39aedce45864ae1067ed44a5551dc973790bb5/source/nodejs/typed-schema/schema/lib/Type.json",
  "description": "Describes a chip for use in a ChipSet.",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for chip.",
      "required": true,
      "version": "1.4"
    },
    "name": {
      "type": "string",
      "description": "Chip name.",
      "required": true,
      "version": "1.4"
    },
    "selected": {
      "type": "boolean",
      "description": "Whether or not the chip is selected. Default value is false",
      "required": true,
      "version": "1.4"
    }
  }
}